#include <stdio.h>
#include <ctype.h>
char s[100];int t=-1;
void p(char c){s[++t]=c;}
char o(){return s[t--];}
int pr(char c){
    if(c=='^')return 3;
    if(c=='*'||c=='/')return 2;
    if(c=='+'||c=='-')return 1;
    return 0;
}
int main(){
    char in[100]="a+b*c",po[100];
    int i,j=0;
    p('(');
    for(i=0;in[i];i++){
        char c=in[i];
        if(isalpha(c))po[j++]=c;
        else if(c=='(')p(c);
        else if(c==')')while(s[t]!='(')po[j++]=o();
        else{while(pr(s[t])>=pr(c))po[j++]=o();p(c);}
    }
    po[j]='\0';
    printf("Postfix: %s",po);
}
