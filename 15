#include<stdio.h>
int h[10]={0};
void add(int k){
    int i=k%10;
    while(h[i])i=(i+1)%10;
    h[i]=k;
}
int find(int k){
    int i=k%10;
    while(h[i]){
        if(h[i]==k)return i;
        i=(i+1)%10;
    }
    return -1;
}
main(){
    add(10);add(20);add(30);add(15);
    printf("Table: ");
    for(int i=0;i<10;i++)printf("%d ",h[i]);
    printf("\n20 at %d\n",find(20));
}
