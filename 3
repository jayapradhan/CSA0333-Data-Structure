#include <stdio.h>

int main() {
    int n;
    long fact = 1;
    
    printf("Enter a non-negative integer: ");
    scanf("%d", &n);
    
    if (n < 0) {
        printf("Factorial is not defined for negative numbers.\n");
    } else {
        for (int i = 1; i <= n; i++) {
            fact *= i;
        }
        printf("Factorial of %d = %ld\n", n, fact);
    }
    
    return 0;
}
