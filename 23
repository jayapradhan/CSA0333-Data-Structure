#include<stdio.h>
#define INF 999
#define V 5
void dijkstra(int g[V][V],int s){
    int d[V],v[V]={0},i,j,u,min;
    for(i=0;i<V;i++)d[i]=INF;
    d[s]=0;
    for(i=0;i<V-1;i++){
        min=INF;
        for(j=0;j<V;j++)
            if(!v[j]&&d[j]<=min)
                min=d[j],u=j;
        v[u]=1;
        for(j=0;j<V;j++)
            if(!v[j]&&g[u][j]&&d[u]!=INF&&d[u]+g[u][j]<d[j])
                d[j]=d[u]+g[u][j];
    }
    printf("Vertex Distance\n");
    for(i=0;i<V;i++)printf("%d \t %d\n",i,d[i]);
}
int main(){
    int g[V][V]={{0,10,0,30,100},
                 {10,0,50,0,0},
                 {0,50,0,20,10},
                 {30,0,20,0,60},
                 {100,0,10,60,0}};
    dijkstra(g,0);
}
