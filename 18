#include<stdio.h>
void q(int a[],int l,int r){
    if(l<r){
        int p=a[r],i=l-1,j,t;
        for(j=l;j<r;j++)if(a[j]<p){i++;t=a[i];a[i]=a[j];a[j]=t;}
        t=a[i+1];a[i+1]=a[r];a[r]=t;
        q(a,l,i);q(a,i+2,r);
    }
}
int main(){
    int a[]={10,7,8,9,1,5},n=6,i;
    q(a,0,n-1);
    for(i=0;i<n;i++)printf("%d ",a[i]);
}
